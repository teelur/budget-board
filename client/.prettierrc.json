{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 90,
  "trailingComma": "es5",
  "plugins": ["prettier-plugin-tailwindcss"],
  "pluginSearchDirs": ["."],
  "importOrder": ["^components/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
